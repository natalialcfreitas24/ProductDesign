    {
      "parameters": {
        "functionCode": "const items = $items();\nconst dadosPesquisa = items[0].json;\n\n// Pega a data do in√≠cio ou start (garante o que foi pesquisado)\nconst dataBase = dadosPesquisa.Inicio || dadosPesquisa.start;\nconst dataConsulta = new Date(dataBase);\n\nconst dia = String(dataConsulta.getDate()).padStart(2, '0');\nconst mes = String(dataConsulta.getMonth() + 1).padStart(2, '0');\nconst ano = dataConsulta.getFullYear();\n\nlet texto = `üìÖ *Agenda do dia ${dia}/${mes}/${ano}*\\n\\n`;\n\n// Filtra itens que n√£o s√£o apenas o header\nconst eventos = items.slice(1).filter(item => {\n  return item.json.summary !== \"Agenda do dia\";\n});\n\nif (eventos.length === 0) {\n  texto += `‚úÖ *N√£o h√° eventos agendados para esta data!*`;\n} else {\n  let eventosFormatados = \"\";\n  for (const item of eventos) {\n    const evento = item.json;\n    const summary = evento.summary || \"Evento sem nome\";\n\n    let start = \"Hor√°rio n√£o informado\";\n    if (evento.start?.dateTime) {\n      start = new Date(evento.start.dateTime).toLocaleTimeString(\"pt-BR\", {\n        hour: \"2-digit\",\n        minute: \"2-digit\",\n        timeZone: \"America/Sao_Paulo\"\n      });\n    }\n\n    let end = \"Hor√°rio n√£o informado\";\n    if (evento.end?.dateTime) {\n      end = new Date(evento.end.dateTime).toLocaleTimeString(\"pt-BR\", {\n        hour: \"2-digit\",\n        minute: \"2-digit\",\n        timeZone: \"America/Sao_Paulo\"\n      });\n    }\n\n    const local = evento.location\n      ? `\\nüìç [Ver no Maps](https://www.google.com/maps/search/?api=1&query=${encodeURIComponent(evento.location)})`\n      : \"\";\n\n    eventosFormatados += `üïí *${start} √†s ${end}*\\nüìå ${summary}${local}\\n\\n`;\n  }\n  texto += eventosFormatados.trim();\n}\n\nreturn [{ json: { mensagem: texto } }];\n"
      },
